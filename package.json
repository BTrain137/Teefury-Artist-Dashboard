{
  "name": "artist-dashboard",
  "version": "1.0.0",
  "description": "Teefury Artist Dashboard",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel ./src -d dist",
    "client": "cd client && npm start",
    "install": "cd client && npm install",
    "local-prod-code": "NODE_ENV=production npm run start:prod",
    "start-prod-server": "NODE_ENV=production nodemon --ignore 'client/*' ./src/index.js --exec babel-node -e js",
    "start-dev-server": "NODE_ENV=dev nodemon --ignore 'client/*' ./src/index.js --exec babel-node -e js",
    "start": "if-env NODE_ENV=production && npm run start:prod || NODE_ENV=dev npm run start:dev",
    "start:dev": "concurrently \"nodemon --ignore 'client/*' ./src/index.js --exec babel-node -e js \" \"npm run client\"",
    "start:prod": "npm run build && cd client && npm run build && cd .. && NODE_ENV=production node ./dist/index.js",
    "test": "mocha --require @babel/register --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btran-teefury/artist-dashboard-2.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/btran-teefury/artist-dashboard-2/issues"
  },
  "homepage": "https://github.com/btran-teefury/artist-dashboard-2#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/register": "^7.7.7",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "concurrently": "^5.0.2",
    "mocha": "^7.0.0",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "if-env": "^1.0.4",
    "mariadb": "^2.2.0",
    "morgan": "^1.9.1"
  }
}
